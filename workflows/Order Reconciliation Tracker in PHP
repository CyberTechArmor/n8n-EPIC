esc',\n  statusSortAsc : false, // track ascending/descending for status sort\n  activeTask    : null\n};\nfunction sortTasks(arr){\n  if(state.sortMode==='dateAsc')  arr.sort((a,b)=>new Date(a.dateReq)-new Date(b.dateReq));\n  if(state.sortMode==='dateDesc') arr.sort((a,b)=>new Date(b.dateReq)-new Date(a.dateReq));\n  if(state.sortMode==='status') {\n    if(state.statusSortAsc) {\n      arr.sort((a,b)=>a.progress.stage - b.progress.stage);\n    } else {\n      arr.sort((a,b)=>b.progress.stage - a.progress.stage);\n    }\n  }\n}\nconst cardContainer = qs('#cardContainer');\nfunction renderCards(){\n  cardContainer.innerHTML='';\n  const filtered = state.tasks.filter(t=>{\n    if(state.filter==='mine'   && t.requester!=='Thomas Stilwell') return false;\n    if(state.filter==='others' && t.requester==='Thomas Stilwell') return false;\n    if(state.patientFilter==='current' && t.patient.id!==state.patientId) return false;\n    if(state.searchTerm && !t.title.toLowerCase().includes(state.searchTerm.toLowerCase())) return false;\n    return true;\n  });\n  sortTasks(filtered);\n  if(!filtered.length){\n    cardContainer.innerHTML='<p class=\"text-gray-500 text-center mt-20\">No tasks found.</p>';\n    return;\n  }\n  filtered.forEach(task=>{\n    const card = el(\\`\n      <article class=\"bg-white rounded-xl border shadow-sm p-4 flex flex-col gap-2 cursor-pointer\n                      hover:ring-2 hover:ring-blue-600 transition\" data-id=\"\\${task.id}\">\n        <header class=\"flex justify-between items-start\">\n          <h3 class=\"font-semibold\">\\${task.title}</h3>\n        </header>\n        <div class=\"text-sm text-gray-600 flex items-center gap-1\">\n          <svg class=\"w-4 h-4 text-gray-400\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\n            <path d=\"M10 0a5 5 0 015 5v1a5 5 0 01-2.038 4H7.038A5 5 0 015 6V5a5 5 0 015-5z\"/>\n            <path fill-rule=\"evenodd\" d=\"M.458 14A2 2 0 012.445 13h15.11a2 2 0 011.986 1.42l.437 1.75A2 2 0 0118 18H2a2 2 0 01-1.978-1.83L.458 14z\" clip-rule=\"evenodd\"/>\n          </svg>\n          \\${task.patient?.name ?? ''} • \\${task.dateReq}\n        </div>\n        <div class=\"flex justify-between items-center\">\n          <p class=\"text-sm text-gray-700\">\\${task.assignee}</p>\n          <span class=\"px-2 py-0.5 rounded-full text-xs font-medium\n                       \\${task.status==='Completed'   ? 'bg-green-100 text-green-700' : ''}\n                       \\${task.status==='In Progress' ? 'bg-blue-100  text-blue-700' : ''}\n                       \\${task.status==='Pending'     ? 'bg-yellow-100 text-yellow-700' : ''}\n                       \\${task.status==='Deferred'    ? 'bg-gray-100 text-gray-700' : ''}\n                       \\${task.status==='Canceled'    ? 'bg-red-100 text-red-700' : ''}\">\n            \\${task.status}\n          </span>\n        </div>\n        \\${dotProgress(task.progress)}\n      </article>\n    \\`);\n    card.onclick = () => openModal(task.id);\n    cardContainer.appendChild(card);\n  });\n}\nconst modal      = qs('#detailModal');\nconst modalTitle = qs('#modalTitle');\nconst tabContent = qs('#tabContent');\nfunction openModal(id){\n  state.activeTask = state.tasks.find(t=>t.id===id);\n  modalTitle.textContent = \\`\\${state.activeTask.title} (\\${state.activeTask.status})\\`;\n  modal.classList.remove('hidden');\n  setTab('details');\n}\nfunction setTab(tab){\n  qsa('[data-tab]').forEach(btn=>{\n    const active = btn.dataset.tab===tab;\n    btn.classList.toggle('border-blue-600',active);\n    btn.classList.toggle('text-blue-600',  active);\n    btn.classList.toggle('border-transparent',!active);\n    btn.classList.toggle('text-gray-500',!active);\n  });\n  const t = state.activeTask;\n  if(tab==='details'){\n    tabContent.innerHTML=\\`\n      <div class=\"grid grid-cols-2 gap-6 text-sm\">\n        <div>\n          <h4 class=\"font-medium mb-1\">Patient Information</h4>\n          <p>\\${t.patient?.name ?? ''}</p><p>ID: \\${t.patient?.id ?? ''} • MRN: \\${t.patient?.mrn ?? ''}</p>\n        </div>\n        <div>\n          <h4 class=\"font-medium mb-1\">Request Information</h4>\n          <p>\\${t.title}</p><p>Created on \\${t.dateReq}</p>\n          <div class=\"mt-2 p-2 bg-gray-100 rounded text-xs border\" style=\"white-space: pre-wrap;\">\n            \\${bbcodeToHtml(t.description)}\n          </div>\n        </div>\n      </div>\n      <div class=\"mt-6\">\n        <h4 class=\"font-medium mb-2\">Progress Timeline</h4>\n        <div class=\"w-full bg-gray-200 h-2 rounded\">\n          <div class=\"bg-blue-600 h-2 rounded\"\n               style=\"width:\\${(t.progress.stage/t.progress.total)*100}%\"></div>\n        </div>\n        <p class=\"text-xs text-gray-500 mt-1\">Stage \\${t.progress.stage} of \\${t.progress.total}</p>\n      </div>\\`;\n  }\n  if(tab==='timeline'){\n    tabContent.innerHTML=t.comments?.map(c=>\\`\n      <div class=\"border-b py-2\">\n        <p class=\"text-sm\"><span class=\"font-medium\">\\${c.author}</span> • \\${c.date}</p>\n        <p class=\"text-gray-700\">\\${c.msg}</p>\n      </div>\\`).join('') ?? '';\n  }\n  if(tab==='comms') tabContent.innerHTML='<p class=\"text-gray-500\">Hook up to Bitrix24 comment thread…</p>';\n  if(tab==='documents') tabContent.innerHTML=t.documents?.length\n    ? t.documents.map(d=>\\`<a href=\"\\${d.url}\" class=\"text-blue-600 underline\">\\${d.name}</a>\\`).join('<br>')\n    : '<p class=\"text-gray-500\">No documents.</p>';\n}\nfunction selectBtn(groupCls, btn){\n  qsa(groupCls).forEach(b=>{\n    const active = b===btn;\n    b.classList.toggle('bg-red-600', groupCls==='.filterBtn' && active);\n    b.classList.toggle('text-white', groupCls==='.filterBtn' && active);\n    b.classList.toggle('bg-white',   !active);\n    b.classList.toggle('text-gray-700',!active);\n    b.classList.toggle('bg-blue-600', groupCls==='.patientBtn' && active);\n    b.classList.toggle('bg-gray-100', groupCls==='.sortBtn' && active);\n    b.classList.toggle('text-gray-900',groupCls==='.sortBtn' && active);\n  });\n}\nqsa('.filterBtn').forEach(btn=>{\n  btn.onclick=()=>{selectBtn('.filterBtn',btn); state.filter=btn.dataset.filter; renderCards();};\n});\nqsa('.patientBtn').forEach(btn=>{\n  btn.onclick=()=>{selectBtn('.patientBtn',btn); state.patientFilter=btn.dataset.patient; renderCards();};\n});\nqsa('.sortBtn').forEach(btn=>{\n  btn.onclick=()=>{                       \n    const sort = btn.dataset.sort;\n    if(sort==='date'){\n      state.sortMode = state.sortMode==='dateDesc' ? 'dateAsc' : 'dateDesc';\n      qs('#dateArrow').textContent = state.sortMode==='dateDesc' ? '▾' : '▴';\n      // Clear status arrow\n      qs('#statusArrow').textContent = '';\n    } else if(sort==='status') {\n      // Toggle status sort asc/desc\n      state.sortMode = 'status';\n      state.statusSortAsc = !state.statusSortAsc;\n      qs('#statusArrow').textContent = state.statusSortAsc ? '▴' : '▾';\n      // Reset date arrow\n      qs('#dateArrow').textContent = '';\n    }\n    selectBtn('.sortBtn',btn);\n    renderCards();\n  };\n});\nqs('#searchInput').oninput=e=>{state.searchTerm=e.target.value; renderCards();};\nqs('#closeModal').onclick=()=>modal.classList.add('hidden');\nmodal.onclick=e=>{if(e.target===modal) modal.classList.add('hidden');};\nqs('#detailTabs').onclick=e=>{if(e.target.dataset.tab) setTab(e.target.dataset.tab);};\n// Initial arrow states on load:\nqs('#dateArrow').textContent = '▾';\nrenderCards();\n</script>\n</body>\n</html>`;\n\n// Return the HTML response from the code node\nreturn [{ json: { html } }];"
