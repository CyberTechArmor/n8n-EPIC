        "jsCode": "/*************************************************\n * n8n Code Node: Generate HTML from Incoming Items\n *************************************************/\n\n// 1) Find the \"task\" item and the \"stage\" items.\nconst taskItem = items.find(item => item.json?.result?.task);\nif (!taskItem) {\n  throw new Error(\"No task data found in incoming items.\");\n}\nconst taskData = taskItem.json.result.task;\n\n// The rest are stage items\nconst stageItems = items.filter(item => !item.json?.result?.task);\n\n// 2) Build stageOrder array by sorting stage items by SORT\n//    and mapping each to { id, label, color }.\nconst stageOrder = stageItems\n  .sort((a, b) => Number(a.json.SORT) - Number(b.json.SORT))\n  .map(item => ({\n    id: item.json.ID,         // e.g. \"1765\"\n    label: item.json.TITLE,   // e.g. \"Acknowledged US\"\n    color: \"#\" + item.json.COLOR\n  }));\n\n// 3) Convert the single task to a request object for display\nfunction mapTaskToRequest(task) {\n  // Example status text mapping\n  let statusText = \"In Progress\";\n  if (task.status === \"5\") statusText = \"Completed\";\n  else if (task.status === \"3\") statusText = \"Pending\";\n\n  // Date from createdDate or fallback\n  const createdDate = (task.createdDate || \"\").split(\"T\")[0] || \"2025-03-27\";\n\n  // Requestor from task.creator.name or fallback\n  const requestor = task.creator?.name || \"Unknown\";\n\n  // Parse patient name from the task description\n  let patientName = \"Unknown\";\n  const matchFirst = task.description.match(/\\[b\\]dem_firstname\\[\\/b\\]:\\s?([^\\n]+)/i);\n  const matchLast  = task.description.match(/\\[b\\]dem_lastname\\[\\/b\\]:\\s?([^\\n]+)/i);\n  if (matchFirst && matchLast) {\n    patientName = matchFirst[1].trim() + \" \" + matchLast[1].trim();\n  } else {\n    patientName = \"Thomas Stilwell\";\n  }\n\n  // Parse MRN\n  let mrn = \"unknown\";\n  const matchMrn = task.description.match(/\\[b\\]mrn\\[\\/b\\]:\\s?(\\S+)/i);\n  if (matchMrn) {\n    mrn = matchMrn[1].trim();\n  }\n\n  // Return structure\n  return {\n    id: Number(task.id) || 0,\n    title: task.title || \"Untitled Task\",\n    patient: patientName,\n    date: createdDate,\n    requestor: requestor,\n    status: statusText,\n    stageId: task.stageId,          // e.g. \"1765\"\n    hasUpdate: false,\n    patientId: task.patientid || \"\",\n    mrn: mrn\n  };\n}\n\n// Build the single request object\nconst singleRequest = mapTaskToRequest(taskData);\n\n// For demonstration, place the same single request in both arrays:\nconst patientRequests = [ singleRequest ];\n// (You can use allRequests if you have more tasks, but here it's the same)\nconst allRequests = [ singleRequest ];\n\n/**\n * Generate final HTML with the new 1/3 and 2/3 layout.\n * - Left column: Patient-specific requests\n * - Right column: Insights & Actions (static examples, or adapt as needed)\n * - Buttons that perform actions = dark grey\n * - Buttons that view details = white w/ border\n */\nconst html = `<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\" />\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\"/>\n  <title>Request Management Dashboard</title>\n  <!-- Tailwind CSS -->\n  <script src=\"https://cdn.tailwindcss.com/3.4.16\"></script>\n  <!-- Google Fonts & Remix Icon -->\n  <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\"/>\n  <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin/>\n  <link href=\"https://fonts.googleapis.com/css2?family=Pacifico&display=swap\" rel=\"stylesheet\"/>\n  <link href=\"https://cdnjs.cloudflare.com/ajax/libs/remixicon/4.6.0/remixicon.min.css\" rel=\"stylesheet\"/>\n\n  <script>\n    // Tailwind config\n    tailwind.config = {\n      theme: {\n        extend: {\n          colors: {\n            primary: '#0EA5E9',\n            secondary: '#64748B'\n          },\n          borderRadius: {\n            'none': '0px',\n            'sm': '4px',\n          