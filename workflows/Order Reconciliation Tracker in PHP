t>\n    /* --------------------------------------------------------------------- *\n     *  SAMPLE DATA – replace with Bitrix24 tasks (e.g., const tasks = items[0].json)\n     * --------------------------------------------------------------------- */\n    const sampleTasks = [\n      {\n        id: 1,\n        direction: 'IB',                       // IB / OB\n        title    : 'Order Reconciliation',\n        status   : 'In Progress',              // Pending | In Progress | Completed\n        patient  : { name: 'Thomas Stilwell', id: 158536, mrn: '502880' },\n        assignee : 'Dr. Sarah Miller',\n        requester: 'Thomas Stilwell',\n        dateReq  : '2025-03-27',\n        progress : { stage: 2, total: 6 },\n        tags     : ['Follow-up'],\n        comments : [\n          { author: 'Dr. Sarah Miller', date: '2025-03-28', msg: 'Updated the status' }\n        ],\n        documents: []\n      },\n      {\n        id: 2,\n        direction: 'OB',\n        title    : 'Medication Review',\n        status   : 'Completed',\n        patient  : { name: 'Thomas Stilwell', id: 158536, mrn: '502880' },\n        assignee : 'Dr. James Wilson',\n        requester: 'Thomas Stilwell',\n        dateReq  : '2025-03-26',\n        progress : { stage: 6, total: 6 },\n        tags     : ['Critical'],\n        comments : [\n          { author: 'Pharmacy', date: '2025-03-27', msg: 'Medication list reconciled' }\n        ],\n        documents: [{ name: 'Med-Review.pdf', url: '#' }]\n      }\n    ];\n\n    /* -------------------- Utilities -------------------------------------- */\n    const qs  = (sel, el=document) => el.querySelector(sel);\n    const qsa = (sel, el=document) => [...el.querySelectorAll(sel)];\n    const $$  = qsa;\n    const el  = html => {\n      const t=document.createElement('template'); t.innerHTML=html.trim();\n      return t.content.firstChild;\n    };\n\n    const dotProgress = ({stage,total}) => {\n      let out='';\n      for(let i=1;i<=total;i++){\n        out += `<span class=\"w-2 h-2 rounded-full ${i<=stage?'bg-blue-600':'bg-gray-300'}\"></span>`;\n      }\n      return `<div class=\"flex gap-1\">${out}</div>`;\n    };\n\n    /* -------------------- App State -------------------------------------- */\n    const state = {\n      tasks          : sampleTasks,            // replace later\n      filter         : 'mine',                 // mine | others\n      patientFilter  : 'all',                  // all | current\n      patientId      : sampleTasks[0]?.patient.id ?? null,  // current patient\n      searchTerm     : '',\n      sortMode       : 'dateDesc',             // dateDesc | dateAsc | status\n      activeTask     : null\n    };\n\n    /* -------------------- Sorting --------------------------------------- */\n    function sortTasks(arr){\n      const list = arr.sort((a,b)=>{\n        if(state.sortMode==='dateAsc')\n          return new Date(a.dateReq) - new Date(b.dateReq);\n        if(state.sortMode==='dateDesc')\n          return new Date(b.dateReq) - new Date(a.dateReq);\n        if(state.sortMode==='status')\n          return a.status.localeCompare(b.status);\n        return 0;\n      });\n      return list;\n    }\n\n    /* -------------------- Render Cards ---------------------------------- */\n    const cardContainer = qs('#cardContainer');\n\n    function renderCards(){\n      cardContainer.innerHTML='';\n      const filtered = state.tasks\n        .filter(t=>{\n          // Mine / Others\n          if(state.filter==='mine'   && t.requester!=='Thomas Stilwell') return false; // adapt to user\n          if(state.filter==='others' && t.requester==='Thomas Stilwell') return false;\n          // Patient scope\n          if(state.patientFilter==='current' && t.patient.id!==state.patientId) return false;\n          // Search\n          if(state.searchTerm &&\n             !t.title.toLowerCase().includes(state.searchTerm.toLowerCase()))\n            return false;\n          return true;\n        });\n\n      sortTasks(filtered);\n\n      if(!filtered.length){\n        cardContainer.innerHTML =\n          '<p class=\"text-gray-500 text-center mt-20\">No tasks found.</p>';\n        return;\n      }\n\n      filtered.forEach(task=>{\n        const dirBadge = `<span class=\"px-1.5 py-0.5 text-[10px] leading-none rounded-full\n                                ${task.direction==='IB'\n                                   ? 'bg-red-600 text-white'\n                                   : 'bg-blue-600 text-white'}\">\n                            ${task.direction}\n                          </span>`;\n\n        const card = el(`\n          <article class=\"bg-white rounded-xl border shadow-sm p-4 flex flex-col gap-2 cursor-pointer\n                          hover:ring-2 hover:ring-blue-600 transition\" data-id=\"${task.id}\">\n            <header class=\"flex justify-between items-start\">\n              <h3 class=\"font-semibold\">${task.title}</h3>\n              ${dirBadge}\n            </header>\n\n            <div class=\"text-sm text-gray-600 flex items-center gap-1\">\n              <svg class=\"w-4 h-4 text-gray-400\" fill=\"currentColor\" viewBox=\"0 0 20 20\">\n                <path d=\"M10 0a5 5 0 015 5v1a5 5 0 01-2.038 4H7.038A5 5 0 015 6V5a5 5 0 015-5z\"/>\n                <path fill-rule=\"evenodd\"\n                      d=\"M.458 14A2 2 0 012.445 13h15.11a2 2 0 011.986 1.42l.437 1.75A2 2 0 0118 18H2a2 2 0 01-1.978-1.83L.458 14z\"\n                      clip-rule=\"evenodd\"/>\n              </svg>\n              ${task.patient.name} • ${task.dateReq}\n            </div>\n\n            <div class=\"flex justify-between items-center\">\n              <p class=\"text-sm text-gray-700\">${task.assignee}</p>\n              <span class=\"px-2 py-0.5 rounded-full text-xs font-medium\n                           ${task.status==='Completed'   && 'bg-green-100 text-green-700'}\n                           ${task.status==='In Progress' && 'bg-blue-100  text-blue-700'}\n                           ${task.status==='Pending'     && 'bg-yellow-100 text-yellow-700'}\">\n                ${task.status}\n              </span>\n            </div>\n\n            ${dotProgress(task.progress)}\n          </article>\n        `);\n        card.addEventListener('click',()=>openModal(task.id));\n        cardContainer.appendChild(card);\n      });\n    }\n\n    /* -------------------- Modal ----------------------------------------- */\n    const modal      = qs('#detailModal');\n    const modalTitle = qs('#modalTitle');\n    const tabContent = qs('#tabContent');\n    const detailTabs = qs('#detailTabs');\n\n    function openModal(id){\n      const task = state.tasks.find(t=>t.id===id);\n      if(!task) return;\n      state.activeTask = task;\n      modalTitle.textContent = `${task.title} (${task.status})`;\n      modal.classList.remove('hidden');\n      setActiveTab('details');\n    }\n\n    function setActiveTab(tab){\n      $$('[data-tab]',detailTabs).forEach(btn=>{\n        const active = btn.dataset.tab===tab;\n        btn.classList.toggle('border-blue-600',active);\n        btn.classList.toggle('text-blue-600', active);\n        btn.classList.toggle('border-transparent',!active);\n        btn.classList.toggle('text-gray-500',   !active);\n      });\n\n      const t = state.activeTask;\n      if(tab==='details'){\n        tabContent.innerHTML=`\n          <div class=\"grid grid-cols-2 gap-6 text-sm\">\n            <div>\n              <h4 class=\"font-medium mb-1\">Patient Information</h4>\n              <p>${t.patient.name}</p>\n              <p>ID: ${t.patient.id} • MRN: ${t.patient.mrn}</p>\n            </div>\n            <div>\n              <h4 class=\"font-medium mb-1\">Request Information</h4>\n              <p>${t.title}</p>\n              <p>Created on ${t.dateReq}</p>\n            </div>\n          </div>\n          <div class=\"mt-6\">\n            <h4 class=\"font-medium mb-2\">Progress Timeline</h4>\n            <div class=\"w-full bg-gray-200 h-2 rounded\">\n              <div class=\"bg-blue-600 h-2 rounded\"\n                   style=\"width:${(t.progress.stage/t.progress.total)*100}%\"></div>\n            </div>\n            <p class=\"text-xs text-gray-500 mt-1\">Stage ${t.progress.stage} of ${t.progress.total}</p>\n          </div>`;\n      }\n      if(tab==='timeline'){\n        tabContent.innerHTML = t.comments.map(c=>`\n          <div class=\"border-b py-2\">\n            <p class=\"text-sm\"><span class=\"font-medium\">${c.author}</span> • ${c.date}</p>\n            <p class=\"text-gray-700\">${c.msg}</p>\n          </div>`).join('');\n      }\n      if(tab==='comms'){\n        tabContent.innerHTML =\n          '<p class=\"text-gray-500\">Hook up to Bitrix24 comment thread…</p>';\n      }\n      if(tab==='documents'){\n        tabContent.innerHTML = t.documents.length\n          ? t.documents.map(d=>`<a href=\"${d.url}\" class=\"text-blue-600 underline\">${d.name}</a>`).join('<br>')\n          : '<p class=\"text-gray-500\">No documents.</p>';\n      }\n    }\n\n    /* -------------------- Event listeners -------------------------------- */\n\n    // Mine / Others\n    $$('[data-filter]').forEach(btn=>{\n      btn.addEventListener('click',()=>{\n        $$('[data-filter]').forEach(b=>b.classList.toggle('bg-red-600',b===btn));\n        $$('[data-filter]').forEach(b=>b.classList.toggle('text-white', b===btn));\n        state.filter = btn.dataset.filter;\n        renderCards();\n      });\n    });\n\n    // Patient scope\n    $$('[data-patient]').forEach(btn=>{\n      btn.addEventListener('click',()=>{\n        $$('[data-patient]').forEach(b=>b.classList.toggle('bg-blue-600',b===btn));\n        $$('[data-patient]').forEach(b=>b.classList.toggle('text-white', b===btn));\n        state.patientFilter = btn.dataset.patient;\n        renderCards();\n      });\n    });\n\n    // Sort\n    qs('#sortSelect').addEventListener('change',e=>{\n      state.sortMode = e.target.value;\n      renderCards();\n    })