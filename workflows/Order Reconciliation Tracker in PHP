-------------------------------*/\n    const state = {\n      tasks      : sampleTasks,          // load real data here\n      filter     : 'mine',\n      searchTerm : ''\n    };\n\n    const cardContainer = qs('#cardContainer');\n\n    function renderCards(){\n      cardContainer.innerHTML = '';\n      const {filter,searchTerm}=state;\n      const filtered = state.tasks.filter(t=>{\n        // ---- FILTER LOGIC -----------------------------\n        if(filter==='mine'       && t.requester!=='Thomas Stilwell') return false;   // ⚑ adapt to current user\n        if(filter==='others'     && t.requester==='Thomas Stilwell') return false;\n        if(filter==='provider'   && !t.assignee)                     return false;\n        // ---- SEARCH -----------------------------------\n        if(searchTerm && !t.title.toLowerCase().includes(searchTerm.toLowerCase()))\n          return false;\n        return true;\n      });\n\n      if(!filtered.length){\n        cardContainer.innerHTML = '<p class=\"text-gray-500 text-center mt-20\">No tasks found.</p>';\n        return;\n      }\n\n      filtered.forEach(task=>{\n        const card = el(`\n          <article class=\"bg-white rounded-xl border shadow-sm p-4 flex flex-col gap-2 cursor-pointer\n                          hover:ring-2 hover:ring-blue-600 transition\" data-id=\"${task.id}\">\n            <header class=\"flex justify-between items-start\">\n              <h3 class=\"font-semibold\">${task.title}</h3>\n              <!-- direction dot -->\n              <span class=\"w-2 h-2 rounded-full ${task.direction==='IB'?'bg-red-600':'bg-blue-600'} mt-1\"></span>\n            </header>\n\n            <!-- meta row -->\n            <div class=\"text-sm text-gray-600 flex items-center gap-1\">\n              <svg class=\"w-4 h-4 text-gray-400\" fill=\"currentColor\" viewBox=\"0 0 20 20\"><path d=\"M10 0a5 5 0 015 5v1a5 5 0 01-2.038 4H7.038A5 5 0 015 6V5a5 5 0 015-5z\"/><path fill-rule=\"evenodd\" d=\"M.458 14A2 2 0 012.445 13h15.11a2 2 0 011.986 1.42l.437 1.75A2 2 0 0118 18H2a2 2 0 01-1.978-1.83L.458 14z\" clip-rule=\"evenodd\"/></svg>\n              ${task.patient.name} • ${task.dateReq}\n            </div>\n            <div class=\"flex justify-between items-center\">\n              <p class=\"text-sm text-gray-700\">${task.assignee}</p>\n              <span class=\"px-2 py-0.5 rounded-full text-xs font-medium\n                           ${task.status==='Completed'   && 'bg-green-100 text-green-700'}\n                           ${task.status==='In Progress' && 'bg-blue-100  text-blue-700'}\n                           ${task.status==='Pending'     && 'bg-yellow-100 text-yellow-700'}\">\n                ${task.status}\n              </span>\n            </div>\n            ${dotProgress(task.progress)}\n          </article>`);\n        card.addEventListener('click',()=>openModal(task.id));\n        cardContainer.appendChild(card);\n      });\n    }\n\n    /* ------------------------------------------------------------------------\n       4. *** MODAL ***\n    -------------------------------------------------------------------------*/\n    const modal         = qs('#detailModal');\n    const modalTitle    = qs('#modalTitle');\n    const tabContent    = qs('#tabContent');\n    const detailTabs    = qs('#detailTabs');\n\n    function openModal(id){\n      const task = state.tasks.find(t=>t.id===id);\n      if(!task) return;\n      state.activeTask = task;\n      modalTitle.textContent = `${task.title} (${task.status})`;\n      modal.classList.remove('hidden');\n      setActiveTab('details');\n    }\n\n    function setActiveTab(tab){\n      $$('[data-tab]',detailTabs).forEach(btn=>{\n        btn.classList.toggle('border-blue-600',btn.dataset.tab===tab);\n        btn.classList.toggle('text-blue-600',  btn.dataset.tab===tab);\n        btn.classList.toggle('border-transparent',btn.dataset.tab!==tab);\n        btn.classList.toggle('text-gray-500',    btn.dataset.tab!==tab);\n      });\n\n      const t = state.activeTask;\n      if(tab==='details'){\n        tabContent.innerHTML=`\n          <div class=\"grid grid-cols-2 gap-6 text-sm\">\n            <div>\n              <h4 class=\"font-medium mb-1\">Patient Information</h4>\n              <p>${t.patient.name}</p>\n              <p>ID: ${t.patient.id} • MRN: ${t.patient.mrn}</p>\n            </div>\n            <div>\n              <h4 class=\"font-medium mb-1\">Request Information</h4>\n              <p>${t.title}</p>\n              <p>Created on ${t.dateReq}</p>\n            </div>\n          </div>\n          <div class=\"mt-6\">\n            <h4 class=\"font-medium mb-2\">Progress Timeline</h4>\n            <div class=\"w-full bg-gray-200 h-2 rounded\">\n              <div class=\"bg-blue-600 h-2 rounded\"\n                   style=\"width:${(t.progress.stage/t.progress.total)*100}%\"></div>\n            </div>\n            <p class=\"text-xs text-gray-500 mt-1\">Stage ${t.progress.stage} of ${t.progress.total}</p>\n          </div>`;\n      }\n      if(tab==='timeline'){\n        tabContent.innerHTML = t.comments.map(c=>`\n          <div class=\"border-b py-2\">\n            <p class=\"text-sm\"><span class=\"font-medium\">${c.author}</span> &bull; ${c.date}</p>\n            <p class=\"text-gray-700\">${c.msg}</p>\n          </div>`).join('');\n      }\n      if(tab==='comms'){\n        tabContent.innerHTML = `<p class=\"text-gray-500\">Hook up to Bitrix24 comment thread…</p>`;\n      }\n      if(tab==='documents'){\n        tabContent.innerHTML = t.documents.length\n          ? t.documents.map(d=>`<a href=\"${d.url}\" class=\"text-blue-600 underline\">${d.name}</a>`).join('<br>')\n          : `<p class=\"text-gray-500\">No documents.</p>`;\n      }\n    }\n\n    /* ------------------------------------------------------------------------\n       5. *** EVENTS ***\n    -------------------------------------------------------------------------*/\n    // Filter buttons\n    $$('[data-filter]').forEach(btn=>{\n      btn.addEventListener('click',()=>{\n        $$('[data-filter]').forEach(b=>b.classList.remove('bg-red-600','text-white'));\n        btn.classList.add('bg-red-600','text-white');\n        state.filter = btn.dataset.filter;\n        renderCards();\n      });\n    });\n\n    // Search\n    qs('#searchInput').addEventListener('input',e=>{\n      state.searchTerm = e.target.value;\n      renderCards();\n    });\n\n    // Tabs\n    detailTabs.addEventListener('click',e=>{\n      if(e.target.dataset.tab) setActiveTab(e.target.dataset.tab);\n    });\n\n    // Close modal\n    qs('#closeModal').addEventListener('click',()=>modal.classList.add('hidden'));\n    modal.addEventListener('click',e=>{\n      if(e.target===modal) modal.classList.add('hidden');  // click backdrop\n    });\n\n    /* ------------------------------------------------------------------------\n       6. *** INITIAL RENDER ***\n    -------------------------------------------------------------------------*/\n    renderCards();\n  </script>\n</body>\n</html>\n",
