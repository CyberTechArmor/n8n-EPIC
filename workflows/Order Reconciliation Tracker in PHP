        "jsCode": "// Receive new Bitrix24 tasks from webhook input\nconst rawTasks = items.map(item => item.json);\n\n// Helpers\n// Map numeric status codes to string status labels for UI\nconst statusLabels = {\n  '2': 'In Progress',\n  '3': 'Completed',\n  '5': 'Pending',\n  '4': 'Deferred',\n  '6': 'Canceled',\n};\n// Parse patient info from 'description' field in bbcode-like format\nfunction parseDescription(desc) {\n  if (!desc) return {};\n  // Create a map of bbcode key:value\n  const info = {};\n  // Match lines starting with [b]key[/b]: value\n  const re = /\\[b\\](.*?)\\[\\/b\\]:\\s*([^\\n\\r]+)/g;\n  let match;\n  while ((match = re.exec(desc))) {\n    const key = match[1].toLowerCase();\n    const val = match[2].trim();\n    info[key] = val;\n  }\n  return info;\n}\n\n// Simple BBCode to HTML converter (handles [b]...[/b] and line breaks)\nfunction bbcodeToHtml(text) {\n  if (!text) return '';\n  return text\n    .replace(/\\[b\\](.*?)\\[\\/b\\]/gi, '<strong>$1</strong>')\n    .replace(/\\n/g, '<br>');\n}\n\n// Calculate progress stage from sorting value (assume max sorting ~ 60000)\nconst MAX_SORTING = 60000;\nconst totalStages = 6;\n\nconst tasks = rawTasks.map(t => {\n  const descriptionInfo = parseDescription(t.description);\n\n  // Build patient name from dem_firstname and dem_lastname if available\n  let patientName = 'Unknown Patient';\n  if (descriptionInfo.dem_firstname || descriptionInfo.dem_lastname) {\n    patientName = \n      (descriptionInfo.dem_firstname ?? '') + \n      (descriptionInfo.dem_lastname ? ' ' + descriptionInfo.dem_lastname : '');\n    patientName = patientName.trim();\n  }\n\n  // Use patientid / mrn or fallback values\n  const patientId = parseInt(descriptionInfo.patientid || 0);\n  const patientMrn = descriptionInfo.mrn || 'N/A';\n\n  // Try to get dateReq from Approximate Date Done or createdDate fallback ISO date only\n  let dateReq = descriptionInfo['approximate date done'] || '';\n  if (!dateReq && t.createdDate) {\n    dateReq = t.createdDate.substring(0,10); // take yyyy-mm-dd\n  }\n\n  // Parse sorting as float, fallback 0\n  const sortNum = parseFloat(t.sorting) || 0;\n  // Scale sortNum to progress stage\n  let progressStage = Math.min(totalStages, Math.max(1, Math.floor((sortNum / MAX_SORTING) * totalStages)));\n  if (!sortNum) progressStage = 1;\n\n  // Map status code to label, fallback to 'Unknown'\n  const statusCode = t.status?.toString() || '';\n  const statusLabel = statusLabels[statusCode] || 'Unknown';\n\n  // Assignee from responsible.name or fallback\n  const assignee = t.responsible?.name || 'Unknown Assignee';\n\n  // Requester from creator.name or fallback\n  const requester = t.creator?.name || 'Unknown Requester';\n\n  // Build title\n  let title = t.title || 'No Title';\n\n  // Tags: could use priority or status label\n  const tags = [];\n  if (t.priority) tags.push('Priority ' + t.priority);\n  if (statusLabel && statusLabel !== 'Unknown') tags.push(statusLabel);\n\n  // Comments & documents arrays empty placeholders\n  const comments = [];\n  const documents = [];\n\n  return {\n    id: parseInt(t.id),\n    title,\n    status: statusLabel,\n    patient: { name: patientName, id: patientId, mrn: patientMrn },\n    assignee,\n    requester,\n    dateReq,\n    progress: { stage: progressStage, total: totalStages },\n    tags,\n    comments,\n    documents,\n    description: t.description || '',\n  };\n});\n\nconst html = `<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\" />\n<meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\" />\n<title>TAC Board</title>\n<script src=\"https://cdn.tailwindcss.com\"></script>\n</head>\n<body class=\"min-h-screen bg-gray-50 flex flex-col\">\n<header class=\"w-full bg-white border-b px-6 py-4 flex flex-wrap gap-4 items-center\">\n  <h1 class=\"text-2xl font-semibold mr-auto\">TAC Board</h1>\n  <div id=\"filterToggle\" class=\"flex rounded-lg overflow-hidden border\">\n    <button data-filter=\"mine\"\n            class=\"filterBtn px-4 py-1 text-sm font-medium bg-red-600 text-white\">\n      Mine\n    </button>\n    <button data-filter=\"others\"\n            class=\"filterBtn px-4 py-1 text-sm font-medium bg-white text-gray-700 hover:bg-gray-100\">\n      Other&nbsp;Provider\n    </button>\n  </div>\n  <div id=\"patientToggle\" class=\"flex rounded-lg overflow-hidden border\">\n    <button data-patient=\"all\"\n            class=\"patientBtn px-3 py-1 text-sm font-medium bg-blue-600 text-white\">\n      All&nbsp;Patients\n    </button>\n    <button data-patient=\"current\"\n            class=\"patientBtn px-3 py-1 text-sm font-medium bg-white text-gray-700 hover:bg-gray-100\">\n      This&nbsp;Patient\n    </button>\n  </div>\n  <div id=\"sortGroup\" class=\"flex rounded-lg overflow-hidden border\">\n    <button data-sort=\"date\"\n            class=\"sortBtn px-3 py-1 text-sm font-medium bg-gray-100 text-gray-900\">\n      Date&nbsp;<span id=\"dateArrow\">▾</span>\n    </button>\n    <button data-sort=\"status\"\n            class=\"sortBtn px-3 py-1 text-sm font-medium bg-white text-gray-700 hover:bg-gray-100 border-l\">\n      Status&nbsp;<span id=\"statusArrow\"></span>\n    </button>\n  </div>\n  <label class=\"relative\">\n    <input id=\"searchInput\" type=\"text\" placeholder=\"Search requests…\"\n           class=\"pl-10 pr-3 py-2 bg-gray-100 rounded-lg focus:outline-none focus:ring w-60\" />\n    <svg class=\"w-5 h-5 absolute left-3 top-1/2 -translate-y-1/2 text-gray-400\"\n         fill=\"currentColor\" viewBox=\"0 0 20 20\">\n      <path fill-rule=\"evenodd\"\n            d=\"M12.9 14.32a8 8 0 111.414-1.414l4.387 4.387a1 1 0 01-1.414 1.414l-4.387-4.387zM14 8a6 6 0 11-12 0 6 6 0 0112 0z\"\n            clip-rule=\"evenodd\" />\n    </svg>\n  </label>\n</header>\n<main id=\"cardContainer\" class=\"p-6 flex flex-col gap-4 max-w-lg mx-auto w-full\"></main>\n<div id=\"detailModal\"\n     class=\"fixed inset-0 bg-black/40 flex items-start jus