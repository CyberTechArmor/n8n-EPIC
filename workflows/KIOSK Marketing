  clearInterval(rotateInterval);\n        loader.style.display = 'none';\n        iframe.style.display = 'block';\n        // only now start idle timers for prompt + reload\n        startIdleTimers(30000);\n      }, { once: true });\n    }\n\n    function startIdleTimers(initialDelay) {\n      clearTimeout(promptTimer);\n      clearTimeout(reloadTimer);\n      promptOverlay.style.display = 'none';\n      // show prompt after `initialDelay`\n      promptTimer = setTimeout(showPrompt, initialDelay);\n      // reload back to first image 15s after prompt\n      reloadTimer = setTimeout(() => location.reload(), initialDelay + 15000);\n    }\n\n    function resetIdleTimers() {\n      // user interacted—give full initialDelay again\n      startIdleTimers(30000);\n    }\n\n    function showPrompt() {\n      // guard: only if we're in scheduling state and iframe is visible\n      if (state === 2 && iframe.style.display === 'block') {\n        promptOverlay.style.display = 'flex';\n      }\n    }\n\n    yesBtn.addEventListener('click', () => {\n      // on “Yes”, give them an extra 60s before re-prompt\n      startIdleTimers(60000);\n    });\n  </script>\n</body>\n</html>\n",
