pace: normal;\n        }\n\n        tr:nth-child(even) {\n            background-color: #f9f9f9;\n        }\n\n        #table-wrapper {\n            max-height: 80vh; /* Set the height for the scrollable table body */\n            overflow-y: auto;\n        }\n    </style>\n</head>\n<body>\n    <h1>SQL Result Viewer</h1>\n    <input type=\"text\" id=\"searchBar\" placeholder=\"Search...\" oninput=\"filterResults()\">\n    \n    <div id=\"results-container\">\n        <div id=\"table-wrapper\">\n            <table id=\"resultsTable\">\n                <thead>\n                    <tr>\n                        ${columnsToDisplay.map(key => `<th>${key}</th>`).join('')}\n                    </tr>\n                </thead>\n                <tbody>\n                    ${data.map(item => `\n                        <tr>\n                            ${columnsToDisplay.map(key => `\n                                <td>\n                                    ${item.json[key] && item.json[key].length > 100 ? `\n                                        <span class=\"expandable\">${item.json[key].substring(0, 100)}... <a href=\"#\" onclick=\"toggleExpand(this)\">Show more</a></span>\n                                        <span class=\"full-content\">${item.json[key]} <a href=\"#\" onclick=\"toggleExpand(this)\">Show less</a></span>\n                                    ` : `${item.json[key] ? item.json[key] : 'N/A'}`}\n                                </td>\n                            `).join('')}\n                        </tr>\n                    `).join('')}\n                </tbody>\n            </table>\n        </div>\n    </div>\n\n    <script>\n        function filterResults() {\n            const query = document.getElementById('searchBar').value.toLowerCase();\n            const rows = document.querySelectorAll('tbody tr');\n            \n            rows.forEach(row => {\n                const cells = row.querySelectorAll('td');\n                const match = Array.from(cells).some(cell => cell.textContent.toLowerCase().includes(query));\n                if (match) {\n                    row.style.display = '';\n                } else {\n                    row.style.display = 'none';\n                }\n            });\n        }\n\n        function toggleExpand(element) {\n            const expandable = element.parentElement.parentElement.querySelector('.expandable');\n            const fullContent = element.parentElement.parentElement.querySelector('.full-content');\n            \n            // Toggle visibility of the expandable and full-content spans\n            if (expandable.style.display === 'none') {\n                expandable.style.display = 'inline';\n                fullContent.style.display = 'none';\n            } else {\n                expandable.style.display = 'none';\n                fullContent.style.display = 'inline';\n            }\n        }\n    </script>\n</body>\n</html>\n`;\n\n// Return the HTML content to be rendered in the \"Respond to Webhook\" node\nreturn [{\n    json: {\n        html: html\n    }\n}];\n"
