TML = `\n                    <div class=\"card-header\">${task.title || 'No Title'}</div>\n                    <div class=\"card-content\">\n                        <div><strong>Task ID:</strong> ${task.taskId || 'N/A'}</div>\n                        <div><strong>Description:</strong> ${task.description || 'No Description'}</div>\n                        <div><strong>Priority:</strong> ${task.priority || 'N/A'}</div>\n                        <div><strong>Status:</strong> ${task.status || 'N/A'}</div>\n                        <div><strong>Created Date:</strong> ${task.createdDate || 'N/A'}</div>\n                        <div><strong>Responsible:</strong> ${task.responsible || 'N/A'}</div>\n                    </div>\n                `;\n                card.addEventListener('click', () => openModal(task.summary.message.content));\n                container.appendChild(card);\n            });\n        }\n\n        // Event listener for search functionality\n        document.getElementById('searchBar').addEventListener('input', (e) => {\n            const searchTerm = e.target.value.toLowerCase();\n            const filteredTasks = tasks.filter(task => \n                Object.values(task).some(value => \n                    String(value).toLowerCase().includes(searchTerm)\n                )\n            );\n            renderCards(filteredTasks);\n        });\n\n        // Function to open modal with task details\n        function openModal(content) {\n            const modal = document.getElementById('taskModal');\n            const modalContent = document.getElementById('modalContent');\n            modalContent.innerHTML = marked.parse(content); // Convert Markdown to HTML\n            modal.style.display = 'flex';\n        }\n\n        // Function to close modal\n        function closeModal(event) {\n            if (event.type === 'keydown' && event.key !== 'Escape') return;\n            const modal = document.getElementById('taskModal');\n            modal.style.display = 'none';\n        }\n\n        // Close modal with Escape key\n        window.addEventListener('keydown', closeModal);\n\n        // Toggle Dark Mode\n        function toggleDarkMode() {\n            document.body.classList.toggle('dark-mode');\n        }\n\n        // Initial render of the dashboard and cards\n        renderDashboard();\n        renderCards(tasks);\n    </script>\n\n</body>\n</html>\n",
